<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="2" parallel="classes">
	<listeners>
		<listener class-name="extendReport.ExtentTestNGIReporterListener"/>
			
		</listeners>
	<test name="ChromeBrowser">
	<parameter name="browserName" value="Chrome"/>
		
    <classes>
     
      <class name="AakashVeloity.LoginPageTestAakash"/>
      <class name="AakashVeloity.HomePageTestAakash"/>

    </classes>
    </test> <!-- Test -->
  <test name="FirefoxTest">
  <parameter name="browserName" value="Firefox"/>
    <classes>
     <class name="AakashVeloity.LoginPageTestAakash"/>
      <class name="AakashVeloity.HomePageTestAakash"/>
    </classes>
        </test> <!-- Test -->

     <test name="EdgeTest">
  <parameter name="browserName" value="Edge"/>
    <classes>
     <class name="AakashVeloity.LoginPageTestAakash"/>
      <class name="AakashVeloity.HomePageTestAakash"/>
    </classes>
    </test> <!-- Test -->
 
 </suite> <!-- Suite -->
